{
  "title": "",
  "description": "",
  "pages": [
    {
      "name": "page1",
      "title": "Who Are You?",
      "elements": [
        {
          "type": "text",
          "name": "name",
          "title": "Your name",
          "isRequired": true
        },
        {
          "type": "text",
          "name": "liveLocation",
          "title": "Where do you live? (used for cost calculation)",
          "isRequired": true
        }
      ]
    },
    {
      "name": "page2",
      "title": "Budget",
      "description": "",
      "elements": [
        {
          "type": "dropdown",
          "name": "budget",
          "title": "Rough budget in USD. Per person, includes flights/travel.",
          "isRequired": true,
          "choices": [
            "< $500",
            "$500 - $1,000",
            "$1,000 - $1,500",
            "$1,500 - $2,000",
            "$2,000 - $2,500",
            "$2,500+"
          ]
        }
      ]
    },
    {
      "name": "page3",
      "title": "Dates",
      "elements": [
        {
          "type": "html",
          "name": "preferredDatesHeader",
          "html": "<h4 style='text-align: left; font-size: 1.2rem; margin-bottom: 10px;'>Preferred Travel Dates</h4><p>Please select date ranges you would prefer to travel</p>"
        },
        {
          "type": "panel",
          "name": "preferredDatesPanel1",
          "title": "Date Range 1",
          "titleLocation": "left",
          "elements": [
            {
              "type": "text",
              "inputType": "date",
              "name": "preferredStartDate1",
              "title": "Start Date",
              "isRequired": true,
              "min": "2023-07-01",
              "validators": [
                {
                  "type": "expression",
                  "expression": "dateCompare({preferredStartDate1}, 'today') >= 0",
                  "text": "Start date cannot be in the past"
                }
              ]
            },
            {
              "type": "text",
              "inputType": "date",
              "name": "preferredEndDate1",
              "title": "End Date",
              "isRequired": true,
              "min": "2023-07-01",
              "validators": [
                {
                  "type": "expression",
                  "expression": "dateCompare({preferredEndDate1}, {preferredStartDate1}) >= 0",
                  "text": "End date cannot be before start date"
                }
              ]
            }
          ]
        },
        {
          "type": "html",
          "name": "addSecondPreferredRangeQuestion",
          "html": "<p>Add another preferred date range?</p>"
        },
        {
          "type": "radiogroup",
          "name": "addSecondPreferredRange",
          "hideTitle": true,
          "choices": [
            {
              "value": true,
              "text": "Yes"
            },
            {
              "value": false,
              "text": "No"
            }
          ],
          "colCount": 2,
          "defaultValue": false
        },
        {
          "type": "panel",
          "name": "preferredDatesPanel2",
          "title": "Date Range 2",
          "titleLocation": "left",
          "elements": [
            {
              "type": "text",
              "inputType": "date",
              "name": "preferredStartDate2",
              "title": "Start Date",
              "min": "2023-07-01",
              "validators": [
                {
                  "type": "expression",
                  "expression": "dateCompare({preferredStartDate2}, 'today') >= 0",
                  "text": "Start date cannot be in the past"
                }
              ]
            },
            {
              "type": "text",
              "inputType": "date",
              "name": "preferredEndDate2",
              "title": "End Date",
              "min": "2023-07-01",
              "validators": [
                {
                  "type": "expression",
                  "expression": "dateCompare({preferredEndDate2}, {preferredStartDate2}) >= 0",
                  "text": "End date cannot be before start date"
                }
              ]
            }
          ],
          "visibleIf": "{addSecondPreferredRange} = true"
        },
        {
          "type": "html",
          "name": "addThirdPreferredRangeQuestion",
          "html": "<p>Add another preferred date range?</p>",
          "visibleIf": "{addSecondPreferredRange} = true"
        },
        {
          "type": "radiogroup",
          "name": "addThirdPreferredRange",
          "hideTitle": true,
          "choices": [
            {
              "value": true,
              "text": "Yes"
            },
            {
              "value": false,
              "text": "No"
            }
          ],
          "colCount": 2,
          "defaultValue": false,
          "visibleIf": "{addSecondPreferredRange} = true"
        },
        {
          "type": "panel",
          "name": "preferredDatesPanel3",
          "title": "Date Range 3",
          "titleLocation": "left",
          "elements": [
            {
              "type": "text",
              "inputType": "date",
              "name": "preferredStartDate3",
              "title": "Start Date",
              "min": "2023-07-01",
              "validators": [
                {
                  "type": "expression",
                  "expression": "dateCompare({preferredStartDate3}, 'today') >= 0",
                  "text": "Start date cannot be in the past"
                }
              ]
            },
            {
              "type": "text",
              "inputType": "date",
              "name": "preferredEndDate3",
              "title": "End Date",
              "min": "2023-07-01",
              "validators": [
                {
                  "type": "expression",
                  "expression": "dateCompare({preferredEndDate3}, {preferredStartDate3}) >= 0",
                  "text": "End date cannot be before start date"
                }
              ]
            }
          ],
          "visibleIf": "{addThirdPreferredRange} = true"
        },
        {
          "type": "html",
          "name": "blackoutDatesHeader",
          "html": "<h4 style='text-align: left; font-size: 1.2rem; margin-bottom: 10px;'>Blackout Dates</h4><p>Please select dates you CANNOT travel</p>"
        },
        {
          "type": "html",
          "name": "hasBlackoutDatesQuestion",
          "html": "<p>Do you have dates you absolutely cannot travel?</p>"
        },
        {
          "type": "radiogroup",
          "name": "hasBlackoutDates",
          "hideTitle": true,
          "choices": [
            {
              "value": true,
              "text": "Yes"
            },
            {
              "value": false,
              "text": "No"
            }
          ],
          "colCount": 2,
          "defaultValue": false
        },
        {
          "type": "panel",
          "name": "blackoutDatesPanel1",
          "title": "Blackout Range 1",
          "titleLocation": "left",
          "elements": [
            {
              "type": "text",
              "inputType": "date",
              "name": "blackoutStartDate1",
              "title": "Start Date",
              "min": "2023-07-01",
              "validators": [
                {
                  "type": "expression",
                  "expression": "dateCompare({blackoutStartDate1}, 'today') >= 0",
                  "text": "Start date cannot be in the past"
                }
              ]
            },
            {
              "type": "text",
              "inputType": "date",
              "name": "blackoutEndDate1",
              "title": "End Date",
              "min": "2023-07-01",
              "validators": [
                {
                  "type": "expression",
                  "expression": "dateCompare({blackoutEndDate1}, {blackoutStartDate1}) >= 0",
                  "text": "End date cannot be before start date"
                }
              ]
            }
          ],
          "visibleIf": "{hasBlackoutDates} = true"
        },
        {
          "type": "html",
          "name": "addSecondBlackoutRangeQuestion",
          "html": "<p>Add another blackout date range?</p>",
          "visibleIf": "{hasBlackoutDates} = true"
        },
        {
          "type": "radiogroup",
          "name": "addSecondBlackoutRange",
          "hideTitle": true,
          "choices": [
            {
              "value": true,
              "text": "Yes"
            },
            {
              "value": false,
              "text": "No"
            }
          ],
          "colCount": 2,
          "defaultValue": false,
          "visibleIf": "{hasBlackoutDates} = true"
        },
        {
          "type": "panel",
          "name": "blackoutDatesPanel2",
          "title": "Blackout Range 2",
          "titleLocation": "left",
          "elements": [
            {
              "type": "text",
              "inputType": "date",
              "name": "blackoutStartDate2",
              "title": "Start Date",
              "min": "2023-07-01",
              "validators": [
                {
                  "type": "expression",
                  "expression": "dateCompare({blackoutStartDate2}, 'today') >= 0",
                  "text": "Start date cannot be in the past"
                }
              ]
            },
            {
              "type": "text",
              "inputType": "date",
              "name": "blackoutEndDate2",
              "title": "End Date",
              "min": "2023-07-01",
              "validators": [
                {
                  "type": "expression",
                  "expression": "dateCompare({blackoutEndDate2}, {blackoutStartDate2}) >= 0",
                  "text": "End date cannot be before start date"
                }
              ]
            }
          ],
          "visibleIf": "{addSecondBlackoutRange} = true"
        },
        {
          "type": "html",
          "name": "addThirdBlackoutRangeQuestion",
          "html": "<p>Add another blackout date range?</p>",
          "visibleIf": "{addSecondBlackoutRange} = true"
        },
        {
          "type": "radiogroup",
          "name": "addThirdBlackoutRange",
          "hideTitle": true,
          "choices": [
            {
              "value": true,
              "text": "Yes"
            },
            {
              "value": false,
              "text": "No"
            }
          ],
          "colCount": 2,
          "defaultValue": false,
          "visibleIf": "{addSecondBlackoutRange} = true"
        },
        {
          "type": "panel",
          "name": "blackoutDatesPanel3",
          "title": "Blackout Range 3",
          "titleLocation": "left",
          "elements": [
            {
              "type": "text",
              "inputType": "date",
              "name": "blackoutStartDate3",
              "title": "Start Date",
              "min": "2023-07-01",
              "validators": [
                {
                  "type": "expression",
                  "expression": "dateCompare({blackoutStartDate3}, 'today') >= 0",
                  "text": "Start date cannot be in the past"
                }
              ]
            },
            {
              "type": "text",
              "inputType": "date",
              "name": "blackoutEndDate3",
              "title": "End Date",
              "min": "2023-07-01",
              "validators": [
                {
                  "type": "expression",
                  "expression": "dateCompare({blackoutEndDate3}, {blackoutStartDate3}) >= 0",
                  "text": "End date cannot be before start date"
                }
              ]
            }
          ],
          "visibleIf": "{addThirdBlackoutRange} = true"
        },
        {
          "type": "text",
          "inputType": "number",
          "name": "minTripDays",
          "title": "Minimum trip length (days)",
          "isRequired": true,
          "min": 1,
          "max": 90
        },
        {
          "type": "text",
          "inputType": "number",
          "name": "maxTripDays",
          "title": "Maximum trip length (days)",
          "isRequired": true,
          "min": 1,
          "max": 90,
          "validators": [
            {
              "type": "expression",
              "expression": "{maxTripDays} >= {minTripDays}",
              "text": "Maximum trip length must be at least minimum trip length"
            }
          ]
        }
      ]
    },
    {
      "name": "page4",
      "title": "Vibe",
      "elements": [
        {
          "type": "checkbox",
          "name": "vibe",
          "title": "",
          "description": "What vibe are you after? Pick up to 3.",
          "choices": [
            "Beach & chill",
            "Adventure/outdoors",
            "Culture/sightseeing",
            "Party/nightlife",
            "Nature/remote",
            "Family-friendly",
            "Wellness/retreat",
            "Expedition (minimal tourist infrastructure)",
            "Foodie"
          ],
          "choicesOrder": "random",
          "showOtherItem": true,
          "maxSelectedChoices": 3,
          "minSelectedChoices": 1
        },
        {
          "type": "radiogroup",
          "name": "moreQuestions",
          "title": "Do you want to fill out a few more questions to help us give you the best possible recommendations?",
          "choices": [
            "Yes",
            {
              "value": "No, save my results and finish",
              "text": "No, save my results and finish survey"
            }
          ]
        }
      ]
    },
    {
      "name": "page5",
      "title": "Past Travel Likes & Dislikes (Optional)\n",
      "elements": [
        {
          "type": "comment",
          "name": "pastLiked",
          "title": "Places you loved"
        },
        {
          "type": "comment",
          "name": "revisit",
          "title": "Are you open to revisiting somewhere you've been?",
          "description": "If so, list which ones"
        },
        {
          "type": "comment",
          "name": "pastDisliked",
          "title": "Places you didn't enjoy"
        }
      ]
    },
    {
      "name": "page6",
      "title": "Wishlist (Optional)",
      "elements": [
        {
          "type": "comment",
          "name": "wishList",
          "title": "Top 3 places on your travel wish list"
        },
        {
          "type": "comment",
          "name": "activities",
          "title": "What activities would you like to do on this trip?"
        }
      ]
    },
    {
      "name": "page7",
      "title": "Final Priorities (Optional) ",
      "elements": [
        {
          "type": "ranking",
          "name": "priorities",
          "title": "Which matters most for you on this trip?",
          "description": "Drag and drop to rank",
          "choices": [
            "Budget",
            "Dates",
            "Vibe",
            "Location",
            "New destination vs. revisit",
            "Activities"
          ]
        }
      ]
    }
  ],
  "triggers": [
    {
      "type": "complete",
      "expression": "{moreQuestions} = 'No, save my results and finish'"
    }
  ],
  "headerView": "advanced"
}